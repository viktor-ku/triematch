{
  "name": "triematch",
  "version": "3.0.2",
  "description": "Fast Trie structure implementation with matching feature",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viktor-ku/triematch.git"
  },
  "keywords": [
    "trie",
    "fast",
    "autocomplete",
    "matching",
    "map",
    "search",
    "tree"
  ],
  "author": "Viktor Kuroljov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viktor-ku/triematch/issues"
  },
  "homepage": "https://github.com/viktor-ku/triematch#readme",
  "engines": {
    "bun": "1.3.1"
  },
  "packageManager": "bun@1.3.1",
  "sideEffects": false,
  "files": [
    "dist/",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "bun.lock"
  ],
  "main": "./dist/cjs/lib.js",
  "module": "./dist/esm/lib.js",
  "types": "./dist/esm/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/lib.d.ts",
      "browser": {
        "import": "./dist/esm/lib.js",
        "require": "./dist/cjs/lib.js"
      },
      "import": "./dist/esm/lib.js",
      "require": "./dist/cjs/lib.js",
      "default": "./dist/esm/lib.js"
    },
    "./package.json": "./package.json"
  },
  "unpkg": "./dist/esm/lib.js",
  "jsdelivr": "./dist/esm/lib.js",
  "scripts": {
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "prepublishOnly": "bun run build",
    "test": "vitest"
  },
  "devDependencies": {
    "typescript": "=5.9.3",
    "vitest": "=4.0.6"
  }
}